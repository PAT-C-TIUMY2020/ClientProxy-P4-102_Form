<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="EndpointService" />
                <binding name="EndpointService1" />
                <binding name="EndpointService2" />
                <binding name="EndpointService3" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:4242/" binding="basicHttpBinding"
                bindingConfiguration="EndpointService" contract="ServiceReference1.IMatematika"
                name="EndpointService" />
            <endpoint address="http://localhost:4242/" binding="basicHttpBinding"
                bindingConfiguration="EndpointService1" contract="ServiceReference2.IMatematika"
                name="EndpointService1" />
            <endpoint address="http://localhost:4242/" binding="basicHttpBinding"
                bindingConfiguration="EndpointService2" contract="ServiceReference3.IMatematika"
                name="EndpointService2" />
            <endpoint address="http://localhost:4242/" binding="basicHttpBinding"
                bindingConfiguration="EndpointService3" contract="ServiceReference4.IMatematika"
                name="EndpointService3" />
        </client>
    </system.serviceModel>
</configuration>